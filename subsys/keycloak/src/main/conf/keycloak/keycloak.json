{
  "realm": "${keycloak.realm}",
  "resource": "${keycloak.serverResource}",
  "realm-public-key": "${keycloak.realmPublicKey}",
  "auth-server-url": "${keycloak.url}",
  "ssl-required": "external",
  "bearer-only": true,
  "enable-cors": true,
  "credentials": {
    "secret": "${keycloak.serverCredentialSecret}"
  }
}